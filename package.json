{
  "name": "university",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "preinstall": "npm install nodemon -g --save",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "deploy": "npm install --silent && npm install -g react-scripts && npm install react-scripts@3.4.1 -g --silent && npm run start"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.18.3",
    "concurrently": "6.0.0",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "mssql": "^6.3.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "objection": "^2.1.3",
    "oracledb": "^5.1.0",
    "pg": "^8.0.3",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "bcrypt": "^5.0.1",
    "concurrently": "6.0.0",
    "express-session": "^1.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^8.3.2"
  }
}
