{
  "name": "university",
  "version": "1.0.0",
  "scripts": {
    "client": "cd client && npm start",
    "server": "nodemon server.js",
    "preinstall": "npm install nodemon -g --save",
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "migrate": "knex --knexfile db/knexfile.js migrate:latest",
    "container": "docker-compose up && migrate"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "6.0.0",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.1",
    "mssql": "^6.3.1",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "objection": "^2.1.3",
    "oracledb": "https://github.com/oracle/node-oracledb/releases/download/v6.3.0/oracledb-src-6.3.0.tgz",
    "pg": "^8.0.3",
    "sqlite3": "^5.0.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "6.0.0",
    "express-session": "^1.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^8.3.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0"
  }
}
