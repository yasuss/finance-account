{
  "name": "university",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:5000/",
  "scripts": {
    "start": "npm run webpack:dev",
    "webpack:build": "NODE_ENV=development webpack --mode=development --config webpack.config.ts --progress",
    "webpack:dev": "NODE_ENV=development webpack-dev-server --mode=development --config webpack.config.ts --progress --host localhost",
    "build": "rimraf ./build && npm run webpack:build",
    "format": "prettier --write \"./**/*.{ts,tsx,json}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "@mui/x-charts": "^6.18.7",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "axios": "^0.21.1",
    "knex": "^0.21.17",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-datepicker": "^4.25.0",
    "react-dom": "^18.2.0",
    "react-inlinesvg": "^4.1.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^6.1.6",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@types/dotenv-webpack": "^7.0.7",
    "@types/lodash": "^4.14.168",
    "@types/react": "^17.0.2",
    "@types/react-datepicker": "^3.1.5",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-vis": "^1.11.6",
    "@types/reflexbox": "^4.0.2",
    "@types/styled-components": "^5.1.7",
    "@types/mini-css-extract-plugin": "^2.5.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "dotenv-webpack": "^8.0.1",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.7.6",
    "react-redux": "^7.2.2",
    "react-table": "^7.6.3",
    "react-tooltip": "^4.2.15",
    "redux": "^4.0.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "@svgr/webpack": "^8.1.0",
    "webpack-dev-server": "^4.15.1",
    "css-loader": "^6.8.1",
    "url-loader": "^4.1.1",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0"
  }
}
